        <font>
            <Font name="Calibri Bold" size="15.0"/>
        </font>
    </Button>
    <ComboBox fx:id="timeBox" layoutX="50.0" layoutY="65.0" prefHeight="31.0" prefWidth="241.0" promptText="Time"
