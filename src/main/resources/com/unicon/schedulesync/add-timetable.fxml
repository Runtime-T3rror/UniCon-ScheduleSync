    <ComboBox fx:id="roomBox" layoutX="50.0" layoutY="160.0" prefHeight="31.0" prefWidth="241.0" promptText="Room"
              style="-fx-background-color: #00ADB5; -fx-text-fill: #eeeeee;"/>
